	sub @e-right' @schwa by @e-right @joint.Eschwa ;
	sub @f-right' @schwa by @f-right @joint.fschwa ;
	sub [ @n-right @x-poste ]' @schwa by [ @n-right @x-poste ] @joint.nschwa ;
	sub @o-right' @schwa by @o-right @joint.oschwa ;
	sub @p-right' @schwa by @p-right @joint.pschwa ;
	sub @q-right' @schwa by @q-right @joint.qschwa ;
