# joints

# introduce joints before glyphs with left side like a
lookup joints-a {
	sub [ @A-right @H-right ]' @a-left by [ @A-right @H-right ] @joint.Ha ;
	sub @CELQZ-right' @a-left by @CELQZ-right @joint.Ea ;
	sub @MKRU-right' @a-left by @MKRU-right @joint.na ;
	sub @e-right' @a-left by @e-right @joint.ea ;
	sub @f-right' [ @a-left @o-left ] by @f-right @joint.fa ;
	sub [ @n-right @x-poste ]' @a-left by [ @n-right @x-poste ] @joint.na ;
	sub @o-right' @a-left by @o-right @joint.oa ;
	sub @q-right' @a-left by @q-right @joint.qa ;
	sub @r-right' @a-left by @r-right @joint.ra ;
	sub [ @s-init @s-right ]' @a-left by [ @s-init @s-right ] @joint.sa ;
	sub @v-right' @a-left by @v-right @joint.va ;
	sub @schwa' @a-left by @schwa @joint.schwaa ;
	} joints-a ;

