languagesystem latn TRK;
languagesystem latn ROM;
languagesystem latn MOL;
languagesystem latn CAT;
languagesystem latn NLD;

# Prefix: Standalone
lookup uc-swash {
	ignore sub @uc-swash-off' @noswash ;
	ignore sub @uc-swash-off' [ idotless i iacute icircumflex idieresis idotaccent igrave imacron iogonek ] @noswash ;
	sub @uc-swash-off' by @uc-swash-on ;
} uc-swash ;

lookup E-isol {
	sub E by E.isol ;
	sub Eacute by Eacute.isol ;
	sub Ebreve by Ebreve.isol ;
	sub Ecaron by Ecaron.isol	;
	sub Ecircumflex by Ecircumflex.isol ;
	sub Edieresis by Edieresis.isol ;
	sub Edotaccent by Edotaccent.isol ;
	sub Egrave by Egrave.isol ;
	sub Emacron by Emacron.isol ;
	sub Eogonek by Eogonek.isol ;
} E-isol ; 

lookup G-isol {
	sub G by G.isol ;
	sub Gbreve by Gbreve.isol ;
	sub Gcommaaccent by Gcommaaccent.isol ;
	sub Gdotaccent by Gdotaccent.isol ;
} G-isol ; 

lookup K-isol {
	sub K by K.isol ;
	sub Kcommaaccent by Kcommaaccent.isol ;
} K-isol ; 

lookup L-isol {
	sub L by L.isol ;
	sub Lacute by Lacute.isol ;
	sub Lcaron by Lcaron.isol ;
	sub Lcommaaccent by Lcommaaccent.isol ;
	sub Lslash by Lslash.isol ;
} L-isol ; 

lookup Q-isol {
	sub Q by Q.isol ;
} Q-isol ; 

lookup R-isol {
	sub R by R.isol ;
	sub Racute by Racute.isol ;
	sub Rcaron by Rcaron.isol ;
	sub Rcommaaccent by Rcommaaccent.isol ;
} R-isol ; 

lookup x-isol {
	sub x by x.isol ;
} x-isol ;

lookup k-salt {
	sub k by k.salt ;
	sub k.medi by k.salt ;
	sub kcommaaccent by kcommaaccent.salt ;
} k-salt ;

lookup germandbls-salt {
	sub germandbls by germandbls.salt ;
} germandbls-salt ;

lookup t-salt {
	sub t by t finial ;
	sub tcaron by tcaron finial ;
	sub tcedilla by tcedilla finial ;
	sub tcommaaccent by tcommaaccent finial ;
} t-salt ;

lookup r-salt {
	sub r by r finial.r ;
	sub racute by racute finial.r ;
	sub rcommaaccent by rcommaaccent finial.r ;
} r-salt ;


lookup descender-salt {
	sub @g by @g-fina ;
	sub @j by @j-fina ;
	sub @y by @y-fina ;
} descender-salt ;

lookup ae-salt {
	sub ae by ae.salt ;
} ae-salt ;

lookup dieresis-salt {
	sub adieresis by adieresis.salt ;
	sub odieresis by odieresis.salt ;
	sub udieresis by udieresis.salt ;
} dieresis-salt ;

lookup odieresis-isol {
	ignore sub odieresis.salt' [ @Uppercase @Lowercase x x.isol ] ;
	ignore sub [ @Uppercase @Lowercase x x.isol ]  odieresis.salt' ;
	sub odieresis.salt' by odieresis.isol ;
} odieresis-isol ;


# sub default form of x to dedicated form if preceded by select glyphs
lookup x-forms {
	sub [ @CELQZ-right @MKRU-right @e-right @n-right @descender-right ] x' by x.poste ;
} x-forms ;

feature ccmp {
# automatic
lookup ccmp_Other_1 {
	@CombiningTopAccents = [acutecomb brevecomb caroncomb circumflexcomb commaturnedabovecomb dieresiscomb dotaccentcomb gravecomb hungarumlautcomb macroncomb ringcomb tildecomb];
	@CombiningNonTopAccents = [cedillacomb ogonekcomb];
	sub [i j]' @CombiningTopAccents by [idotless jdotless];
	sub [i j]' @CombiningNonTopAccents @CombiningTopAccents by [idotless jdotless];
	@Markscomb = [commaaccentcomb dieresiscomb gravecomb acutecomb hungarumlautcomb circumflexcomb caroncomb brevecomb ringcomb tildecomb macroncomb cedillacomb];
	@MarkscombCase = [commaaccentcomb.case dieresiscomb.case gravecomb.case acutecomb.case hungarumlautcomb.case circumflexcomb.case caroncomb.case brevecomb.case ringcomb.case tildecomb.case macroncomb.case cedillacomb.case];
	sub @Markscomb @Markscomb' by @MarkscombCase;
	sub @Uppercase @Markscomb' by @MarkscombCase;
} ccmp_Other_1;

lookup ccmp_Other_2 {
	sub @Markscomb' @MarkscombCase by @MarkscombCase;
	sub @MarkscombCase @Markscomb' by @MarkscombCase;
} ccmp_Other_2;
} ccmp;

feature locl {
# automatic
lookup locl_latn_0 {
	script latn;
	language AZE;
	sub i by idotaccent;
	language CRT;
	sub i by idotaccent;
	language KAZ;
	sub i by idotaccent;
	language TAT;
	sub i by idotaccent;
	language TRK;
	sub i by idotaccent;
} locl_latn_0;

lookup locl_latn_1 {
	script latn;
	language ROM;
	sub Scedilla by Scommaaccent;
	sub scedilla by scommaaccent;
	sub Tcedilla by Tcommaaccent;
	sub tcedilla by tcommaaccent;
	language MOL;
	sub Scedilla by Scommaaccent;
	sub scedilla by scommaaccent;
	sub Tcedilla by Tcommaaccent;
	sub tcedilla by tcommaaccent;
} locl_latn_1;

lookup locl_latn_2 {
	script latn;
	language CAT;
	sub l periodcentered' l by periodcentered.loclCAT;
	sub L periodcentered' L by periodcentered.loclCAT;
} locl_latn_2;

lookup locl_latn_3 {
	script latn;
	language NLD;
	sub iacute j' by jacute;
	sub Iacute J' by Jacute;
} locl_latn_3;
} locl;

feature dlig {
sub lslash lslash by lslash_lslash ;

lookup wordliga {
sub Y e a h exclam by Y_e_a_h_exclam ;
sub B r a v o exclam exclam exclam by B_r_a_v_o_exclam_exclam_exclam ;
sub s e h r space g u t exclam by s_e_h_r_g_u_t_exclam ;
} wordliga ;
} dlig;

feature calt {
# dedicated case forms of punctuation 

sub @figures @punct-dflt' @figures by @punct-case ;
sub @Uppercase @punct-dflt' @Uppercase by @punct-case ;

# -------------------------------------------------------------------------------
# dedicated positional forms
#

# sub default g by g.isol unless preceded or followed by other alphabetic character

lookup g-isol {
ignore sub @g' [ @Uppercase @Lowercase x x.isol ] ;
ignore sub [ @Uppercase @Lowercase x x.isol ] @g' ;
	sub @g' by @g-isol ;
} g-isol ;

# sub default o and non-joining o by o.isol unless preceded or followed by other alphabetic characters
lookup o-isol {
	ignore sub @o' [ @Uppercase @Lowercase x x.isol ] ;
	ignore sub [ @Uppercase @Lowercase x x.isol ]  @o' ;
	sub @o' by @o-isol ;
} o-isol ;

# sub default forms of select glyphs to dedicated forms if followed by e
lookup pre-e-forms {
	sub [ b b.medi @o ]' [ @e-left @ascender-left ] by [ b.e b.medi.e @o-e ] ;
	sub [ @GJ @g @g-medi @j @y ]' @e-left by [ @GJ-e @g-e @g-medi-e @j-e @y-e ] ;
} pre-e-forms ;

# sub default forms of e to dedicated forms if preceded by select glyphs
lookup e-forms {
	sub [ @A-right @H-right @b-right @o-right @r-right @v-right @schwa ] @e' by @e-postb ;
} e-forms ;

# sub default forms of t to dedicated forms if preceded by r
lookup t-forms {
	sub @r-right @t' by @t-r ;
} t-forms ;

# sub default form of eth to dedicated form if preceded by select glyphs
lookup eth-forms {
	sub [ @o-right @r-right @v-right ] eth' by eth.posto ;
} eth-forms ;

# sub initial forms of lowercase glyphs to medial (connecting) forms if preceded by select glyphs
lookup medi {
	ignore sub [ instroke.ascender @nonjoiner @g-fina @j-fina @y-fina ] @init' ;
	sub [ @CELQZ-right @GJ @GJ-e @MKRU-right @Lowercase ] @init' by @medi ;
	sub [ @A-right @H-right ] [ @a-init ]' by [ @a-medi ] ;
	sub @I @l-init' by instroke.ascender @l-medi ;
	sub periodcentered.loclCAT @l-init' by @l-medi ;
} medi ;

lookup medi2 {
	sub @s-isol' [ @Lowercase x ] by @s-init ;
} medi2 ;

