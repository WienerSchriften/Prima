feature kern {
lookup kern-calt {
# asterisk
	pos f' finial.f' 300 asterisk ; 
	pos f.medi' finial.f' 150 asterisk ;
	pos [ l l.medi ]' finial' 180 asterisk ;
	pos [ lcaron lcaron.medi dcaron dcaron.medi tcaron ]' finial' 300 asterisk ;
# quotes
	pos f' finial.f' 280 [ quoteleft quotedblleft ] ; 
	pos f.medi' finial.f' 130 [ quoteleft quotedblleft ] ;
	pos [ l l.medi ]' finial' 150 [ quoteleft quotedblleft ] ;
	pos [ lcaron lcaron.medi dcaron dcaron.medi tcaron ]' finial' 250 [ quoteleft quotedblleft ] ;
	pos f' finial.f' 230 [ quoteright quotedblright ] ; 
	pos f.medi' finial.f' 100 [ quoteright quotedblright ] ;
	pos [ l l.medi ]' finial' 80 [ quoteright quotedblright ] ;
	pos [ lcaron lcaron.medi dcaron dcaron.medi tcaron ]' finial' 200 [ quoteright quotedblright ] ;
} kern-calt ;
} kern;

