 	} joints-schwa ;


# introduce joints after descenders
lookup joints-ga {
	sub @descender-right' [ @a-left @o-left ] by @descender-right @joint.ga ;
} joints-ga ;

# add outstroke after uppercase when not joining or isolated
lookup unjoined-uc {
	ignore sub [ @A-right @H-right ]' [ @a-left @e-left @n-left @o-left @v-left @x-left @joint.Ha @joint.He @joint.Hn @joint.Hv @joint.Ho ] ;
	sub @A-right' by @A-right finial.A ;
	sub @H-right' by @H-right finial.H ;
} unjoined-uc ;

# add outstroke after joining uc and lc unless followed by other lc glyphs
lookup finial {
	ignore sub [ @A-right @H-right @CELQZ-right @MKRU-right @e-right @f-right @n-right @r-right @t-right @x-poste @uc-swash-on ]' [ @Lowercase @joints-all @finial.all ] ;
	sub [ @e-right @n-right @t-right @x-poste ]' by [ @e-right @n-right @t-right @x-poste ] finial ;
	sub @f-right' by @f-right finial.f ;
	sub @r-right' by @r-right finial.r ;
	sub [ @CELQZ-right ]' by [ @CELQZ-right ] finial.uc ;
	sub [ @MKRU-right ]' by [ @MKRU-right ] finial.uc.short ;
} finial ;

# add outstroke after joining uc and lc if followed by stylistic alternates
lookup finial_explicit {
	sub [ @e-right @n-right @t-right ]' [ @g-fina @j-fina @k-salt @y-fina ] by [ @e-right @n-right @t-right ] finial ;
	sub @f-right' [ @g-fina @j-fina @k-salt @y-fina ] by @f-right finial.f ;
	sub @j' [ @g-fina @j-fina @k-salt @y-fina ] by @j-fina ;
	sub @g' [ @g-fina @j-fina @k-salt @y-fina ] by @g-fina ;
	sub @g-medi' @k-salt by @g-fina ;
	sub @r-right' [ @g-fina @j-fina @k-salt @y-fina ] by @r-right finial.r ;
	sub [ @CELQZ-right ]' [ @g-fina @j-fina @k-salt @y-fina ] by [ @CELQZ-right ] finial.uc ;
	sub [ @MKRU-right ]' [ @g-fina @j-fina @k-salt @y-fina ] by [ @MKRU-right ] finial.uc.short ;
	sub @y' [ @g-fina @j-fina @k-salt @y-fina ] by @y-fina ;
} finial_explicit ;

# use dedicated final forms of selected glyphs unless followed by other lc glyphs
lookup final {
	ignore sub  [ @GJ @g @g-medi @j @r @y ]' [ @Lowercase @joints-all @finial.all ] ;
	sub @g' by @g-fina ;
	sub @g-medi' by @g-fina ;
